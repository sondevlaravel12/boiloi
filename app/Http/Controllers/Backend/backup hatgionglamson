<script>

    $(function(){

        $('.navbar-nav li a').click(function(){

            var tabid = $(this).attr('rel');



            jQuery.ajax({

                url:  '/common/get_product_tab',

                data: {"tabid":tabid},

                type: 'POST',

                dataType: 'json',

                success: function(data){

                    if( data.status == 1 ){

                        $('#tab-'+tabid+' .box-right .product-list').html( data.html );

                    }

                }

            });



        })

    })

</script>

